{
  "name": "analytics-monorepo",
  "private": true,
  "packageManager": "npm@10.9.0",
  "workspaces": [
    "apps/*",
    "services/*",
    "prisma",
    "scripts"
  ],
  "scripts": {
    "dev:api": "turbo run dev --filter=api",
    "dev:web": "turbo run dev --filter=web",
    "dev:vanna": "python services/vanna/app.py",
    "build": "turbo run build",
    "lint": "turbo run lint",
    "seed": "tsx scripts/seed.ts"
  },
  "devDependencies": {
    "@types/node": "^22.9.0",
    "prisma": "^6.0.1",
    "tsx": "^4.19.2",
    "turbo": "^2.3.0",
    "typescript": "^5.6.3"
  },
  "dependencies": {
    "@prisma/client": "^6.0.1"
  }
}